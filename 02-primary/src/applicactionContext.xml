<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 声明bean:让spring创建对象
         id:对象的名称，是唯一值。 和变量的命名规则相同。
         class:类的全限定名称(不能是接口)
         
         <bean>的作用等价于
         SomeService someService = new com.bjpowernode.service.SomeServiceImpl();
    -->
    <bean id="someService" class="com.bjpowernode.service.SomeServiceImpl" />
    
    <bean id="someService1" class="com.bjpowernode.service.SomeServiceImpl" />
   
</beans>

<!-- spring的配置文件 
          命名空间
-->